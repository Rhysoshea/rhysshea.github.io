
---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Rhys Shea's Blog</title>
	<link href="https://programming.rhysshea.com/blog/atom.xml" rel="self"/>
	<link href="https://programming.rhysshea.com/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://programming.rhysshea.com/blog</id>
	<author>
		<name>Rhys Shea</name>
		<email>rhys.shea.10@ucl.ac.uk</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://programming.rhysshea.com{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://programming.rhysshea.com{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
